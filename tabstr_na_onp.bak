program odwr_not_pol;

uses unit_rownanie_na_struktury, unit_kolejki_kopce; {* dolacza jednostke z deklaracja typow danych, struktur oraz funkcja zamieniajaca
                                napis na tablice struktur(rozbij_na_str(rownanie, wynik, licznik): Boolean *}


var
    rownanie:       string;
    tablica_str:    tablica;
    ile_str:        integer;
    stan:           Boolean;

    P_kolejki: ^element_kolejki;   { glowa kolejki }
    P_kopca:   ^element_kopca;     { glowa kopca }


begin
rownanie:='-sin(a+b)';

stan:=rozbij_na_str(rownanie, tablica_str, ile_str);

if (stan ) then
    begin
    writeln('utworzono ',ile_str,' struktur');
    end
else
    begin
    writeln('Nie udalo sie utworzyc struktur danych');
    end;
end.
